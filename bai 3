//HOAN DOI 2 SO BANG CON TRO 
#include<iostream> 
#include<conio.h>
#include <cmath>
using namespace std;
void swap (int *p1,int *p2){
	int temp;
	temp=*p1;
	*p1=*p2;
	*p2=temp;
}
int main (){
	int temp;
	int p1,p2;
	cout<<" nhap 2 so ";
	cin>>p1>>p2;
	swap(&p1,&p2);
	cout<<" sau "<<p1 << p2<< "  ";
	
	return 0;
}
CONG 2 SO BANG CON TRO 
#include<iostream> 
#include<conio.h>
#include <cmath>
using namespace std;
int main (){
	int *p1,*p2;
	int a,b;
	cin >>a>>b;
	p1=&a;
	p2=&b;
	cout<<*p1+*p2;
	return 0;
}

